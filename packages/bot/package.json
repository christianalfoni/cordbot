{
  "name": "@cordbot/agent",
  "version": "6.6.0",
  "description": "Discord bot powered by Claude Agent SDK",
  "type": "module",
  "bin": {
    "cordbot": "./bin/cordbot.js"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc && node scripts/copy-skills.js && chmod +x bin/cordbot.js",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.js",
    "copy-readme": "node scripts/copy-readme.js",
    "prepublishOnly": "npm run copy-readme && npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [
    "discord",
    "claude",
    "bot",
    "ai",
    "coding",
    "assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.2.23",
    "@types/express": "^5.0.6",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "chrono-node": "^2.9.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "googleapis": "^128.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.11.0",
    "nanoid": "^5.0.0",
    "node-cron": "^3.0.3",
    "turndown": "^7.2.2",
    "unpdf": "^1.4.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/turndown": "^5.0.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
